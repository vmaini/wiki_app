<h1>Write and Publish a New Wikiful Article!</h1>
<% if @article.errors.any? %>
  <div class="error_messages">
    <h2>You're missing some field</h2>
    <ul>
      <% @article.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
    </ul>
  </div>
<% end %>
<%= render :partial => 'form' %>